
import math
import os
import random
import re
import sys



if __name__ == '__main__':
    n = int(input().strip())
    is_even = [str(i) for i in range(0, 1000, 2)] 

    
    if str(n) in is_even:
        if 2 <= n <= 5:
            print('Not Weird')
        elif 6 <= n <= 20:
            print('Weird')
        elif n > 20:
            print('Not Weird')
    else:
        print('Weird')
